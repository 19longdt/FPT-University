,[PostalCode],[Country],[PhoneNumber], CurrencyCode, SpecialOfferID, [DiscountPct],[Type],[MinQty] ,[MaxQty],[StartDate],[EndDate],
OrderDate, ShipDate, DueDate, CountryName, TerritoryID, GroupName, RegionName
